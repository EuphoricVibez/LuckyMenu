local function MDDNBIW_fake_script() -- buttonDeleteNature.onClick 
	local script = Instance.new('LocalScript', buttonDeleteNature)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	
	
	local function onButtonActivated()
		for i, j in pairs(game.Workspace:GetChildren()) do -- remove trees
			if  j.Name == "Tree" then
				j:Destroy()
			end
		end
		for i, j in pairs(game.Workspace:GetChildren()) do -- remove grass
			if  j.Name == "Grass" then
				j:Destroy()
			end
		end
		for i, j in pairs(game.Workspace:GetChildren()) do -- remove flowers
			if  j.Name == "Flower" then
				j:Destroy()
			end
		end
		for i, j in pairs(game.Workspace:GetChildren()) do -- remove stems
			if  j.Name == "Stem" then
				j:Destroy()
			end
		end
		for i, j in pairs(game.Workspace:GetChildren()) do -- remove stamen
			if  j.Name == "Stamen" then
				j:Destroy()
			end
		end
	
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(MDDNBIW_fake_script)()
local function GAHVXOX_fake_script() -- buttonPipeTopsSolid.onClick 
	local script = Instance.new('LocalScript', buttonPipeTopsSolid)

	local button = script.Parent
	
	
	
	local function onButtonActivated()
		
		for i, j in pairs(game:GetService("Workspace").CenterBlocks.Givers.BlockGiverGalaxy2:GetChildren()) do
			if  j.Name == "Pipe" then
				j.CanCollide = true
			end
		end
		
		for i, j in pairs(game:GetService("Workspace").CenterBlocks.Givers.BlockGiverGalaxy1:GetChildren()) do
			if  j.Name == "Pipe" then
				j.CanCollide = true
			end
		end
		
		for i, j in pairs(game:GetService("Workspace").CenterBlocks.Givers.BlockGiverRainbow1:GetChildren()) do
			if  j.Name == "Pipe" then
				j.CanCollide = true
			end
		end
		
		for i, j in pairs(game:GetService("Workspace").CenterBlocks.Givers.BlockGiverRainbow2:GetChildren()) do
			if  j.Name == "Pipe" then
				j.CanCollide = true
			end
		end
		
		for i, j in pairs(game:GetService("Workspace").CenterBlocks.Givers.VoidGiver:GetChildren()) do
			if  j.Name == "Pipe" then
				j.CanCollide = true
			end
		end
		--game:GetService("Workspace").CenterBlocks.Givers.BlockGiverGalaxy2.Pipe.CanCollide = true
	
		--game:GetService("Workspace").CenterBlocks.Givers.VoidGiver.Pipe.CanCollide = true	
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(GAHVXOX_fake_script)()
