-- Scripts:

local function EYHCTCV_fake_script() -- buttonGalaxySpawn.onClick 
	local script = Instance.new('LocalScript', buttonGalaxySpawn)

	local button = script.Parent
	
	local function onButtonActivated()
			game:GetService("ReplicatedStorage").SpawnGalaxyBlock:FireServer()
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(EYHCTCV_fake_script)()
local function SSFIQY_fake_script() -- main.draggable 
	local script = Instance.new('LocalScript', main)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(SSFIQY_fake_script)()
local function ENQLNIM_fake_script() -- buttonEmptyBackpack.onClick 
	local script = Instance.new('LocalScript', buttonEmptyBackpack)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	
	local function onButtonActivated()
		for _, tool in ipairs(player.Backpack:GetChildren()) do
			if tool:IsA("Tool") then
				tool:Destroy()
			end
		end --Deletes all tools from Backpack
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(ENQLNIM_fake_script)()
local function LGUT_fake_script() -- buttonRainbowSpawn.onClick 
	local script = Instance.new('LocalScript', buttonRainbowSpawn)

	local button = script.Parent
	
	
	local function onButtonActivated()
		game.ReplicatedStorage.SpawnRainbowBlock:FireServer();
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(LGUT_fake_script)()
local function SJYDQO_fake_script() -- buttonExtraMenu.onClick 
	local script = Instance.new('LocalScript', buttonExtraMenu)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local frameExtraMenu = script.Parent.Parent.Parent.extraMenu
	local frameCredits = frameExtraMenu.credits
	
	local function onButtonActivated()
		if frameExtraMenu.Visible == true then
			frameExtraMenu.Visible = false
			frameCredits.Visible = false
		else
			frameExtraMenu.Visible = true
		end
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(SJYDQO_fake_script)()
local function IWHO_fake_script() -- buttonTeleToMid.onClick 
	local script = Instance.new('LocalScript', buttonTeleToMid)

	local button = script.Parent
	local player = game.Players.LocalPlayer
	local midPosition = Vector3.new(-1041, 205, 90)
	
	
	local function onButtonActivated()
		player.Character:MoveTo(midPosition)
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(IWHO_fake_script)()
local function SVQDC_fake_script() -- buttonToggleVis.onClick 
	local script = Instance.new('LocalScript', buttonToggleVis)

	local button = script.Parent
	local frameMain = script.Parent.Parent.Parent.main
	local frameExtraMenu = script.Parent.Parent.Parent.extraMenu
	local frameCredits = frameExtraMenu.credits
	
	local function onButtonActivated()
		if frameMain.Visible == true then --close all open menus
			frameMain.Visible = false
			frameExtraMenu.Visible = false
			frameCredits.Visible = false
			button.Text = "Open"
		else
			frameMain.Visible = true
			button.Text = "Close"
		end
	end
	
	button.Activated:Connect(onButtonActivated)
end
coroutine.wrap(SVQDC_fake_script)()
